<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="component" xsi:type="string">Webkul_MpAdvancedBookingSystem/js/form</item>
        </item>
    </argument>
    <settings>
        <buttons>
            <button name="save" class="Webkul\MpAdvancedBookingSystem\Block\Adminhtml\Catalog\Product\Edit\Button\Save"/>
        </buttons>
    </settings>
    <fieldset name="product-details">
        <field name="language_seniority">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">text</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="component" xsi:type="string">Vendor_Module/js/form/element/language-seniority</item>
                    <item name="label" xsi:type="string" translate="true">Language and Seniority</item>
                    <item name="dataScope" xsi:type="string">language_seniority</item>
                    <item name="sortOrder" xsi:type="number">100</item>
                </item>
            </argument>
        </field>
        <field name="skill_seniority">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">text</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="component" xsi:type="string">Vendor_Module/js/form/element/skill-seniority</item>
                    <item name="label" xsi:type="string" translate="true">Skill and Seniority</item>
                    <item name="dataScope" xsi:type="string">skill_seniority</item>
                    <item name="sortOrder" xsi:type="number">100</item>
                </item>
            </argument>
        </field>
    </fieldset>
</form>
