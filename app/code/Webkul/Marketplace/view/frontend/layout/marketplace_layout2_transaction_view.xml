<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="seller-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="seller.page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Transactions Details</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="seller.content">
            <block class="OrionAlliance\NewModule\Block\Transaction\View" name="marketplace_transaction_view" template="OrionAlliance_NewModule::transaction/view.phtml" cacheable="false">
                <arguments>
                    <argument name="view_model" xsi:type="object">OrionAlliance\NewModule\ViewModel\HelperViewModel</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
