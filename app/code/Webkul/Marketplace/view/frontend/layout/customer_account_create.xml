<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
           <css src="OrionAlliance_NewModule::css/wk_block.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <referenceBlock name="customer_form_register" remove="true" />
            <block class="OrionAlliance\NewModule\Block\RegistrationForm" name="registration_form" template="OrionAlliance_NewModule::registration_form.phtml"/>
            <block class="OrionAlliance\NewModule\Block\Sellerregistration" name="seller_form_register" template="OrionAlliance_NewModule::confpartner.phtml" ifconfig="marketplace/landingpage_settings/allow_seller_registration_block">
