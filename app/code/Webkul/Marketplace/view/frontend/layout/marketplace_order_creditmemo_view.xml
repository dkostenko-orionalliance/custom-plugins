<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="marketplace_order_creditmemo_view_totals"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">View Memo</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="OrionAlliance\NewModule\Block\Order\View"  name="marketplace_order_creditmemo_view" template="OrionAlliance_NewModule::order/creditmemo/view.phtml" cacheable="false">
                <block class="OrionAlliance\NewModule\Block\Order\Creditmemo\Items" name="marketplace_order_creditmemo_items" template="OrionAlliance_NewModule::order/creditmemo/items.phtml" />
                <arguments>
                    <argument name="view_model" xsi:type="object">OrionAlliance\NewModule\ViewModel\HelperViewModel</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
