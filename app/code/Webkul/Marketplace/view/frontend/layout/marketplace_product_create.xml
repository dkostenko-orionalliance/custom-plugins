<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <update handle="marketplace_styles"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Marketplace Add New Product</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="OrionAlliance\NewModule\Block\Product\Create" name="marketplace_product_create" template="OrionAlliance_NewModule::product/create.phtml" cacheable="false">
             <arguments>
                    <argument name="view_model" xsi:type="object">OrionAlliance\NewModule\ViewModel\HelperViewModel</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
