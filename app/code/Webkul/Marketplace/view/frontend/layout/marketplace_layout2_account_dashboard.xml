<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="seller-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="seller.page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Dashboard</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="seller.content">
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard" name="marketplace_account_dashboard" template="OrionAlliance_NewModule::layout2/account/dashboard.phtml" cacheable="false">
            <arguments>
                    <argument name="view_model" xsi:type="object">OrionAlliance\NewModule\ViewModel\HelperViewModel</argument>
                </arguments>
            </block>
        </referenceContainer>

        <referenceContainer name="marketplace_account_dashboard">
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard\Diagrams" name="diagrams" template="OrionAlliance_NewModule::account/diagrams.phtml" cacheable="false"/>
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard\LocationChart" name="locationChart" template="OrionAlliance_NewModule::account/locationChart.phtml" cacheable="false"/>
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard\ReviewChart" name="reviewPriceChart" template="OrionAlliance_NewModule::layout2/account/dashboard/reviewPriceChart.phtml" cacheable="false"/>
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard\ReviewChart" name="reviewValueChart" template="OrionAlliance_NewModule::layout2/account/dashboard/reviewValueChart.phtml" cacheable="false"/>
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard\ReviewChart" name="reviewQualityChart" template="OrionAlliance_NewModule::layout2/account/dashboard/reviewQualityChart.phtml" cacheable="false"/>
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard\CategoryChart" name="categoryChart" template="OrionAlliance_NewModule::account/categoryChart.phtml" cacheable="false"/>
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard" name="marketplace_dashboard_totals" template="OrionAlliance_NewModule::layout2/account/dashboard/totals.phtml" cacheable="false"/>
            <block class="OrionAlliance\NewModule\Block\Page\Notifications" name="marketplace_dashboard_notifications" template="OrionAlliance_NewModule::layout2/account/dashboard/notifications.phtml" cacheable="false"/>
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard" name="marketplace_dashboard_latest_orders" template="OrionAlliance_NewModule::layout2/account/dashboard/latest_orders.phtml" cacheable="false">
                <uiComponent name="marketplace_latest_orders_listing"/>
            </block>
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard" name="marketplace_dashboard_customer_review" template="OrionAlliance_NewModule::layout2/account/dashboard/review.phtml" cacheable="false"/>
            <block class="OrionAlliance\NewModule\Block\Account\Dashboard" name="dashboard_additional_content_block" template="OrionAlliance_NewModule::account/dashboard/additional_content.phtml" cacheable="false"/>
        </referenceContainer>

        <referenceBlock name="marketplace_account_dashboard">
            <arguments>
                <argument name="view_model" xsi:type="object">OrionAlliance\NewModule\ViewModel\HelperViewModel</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
